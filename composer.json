{
    "name": "terehinis/paypal-bundle",
    "type": "symfony-bundle",
    "description": "Simple bundle for Paypal integration",
    "keywords": ["Paypal", "payment"],
    "homepage": "https://github.com/bee-lab/",
    "license": "LGPL-3.0-or-later",
    "require": {
        "php": ">=7.0",
        "doctrine/orm": "^2.3",
        "omnipay/paypal": "^2.0|^3.0",
        "symfony/config": "^2.8|^3.3|^4.0",
        "symfony/dependency-injection": "^2.8|^3.3|^4.0",
        "symfony/expression-language": "^2.8|^3.3|^4.0",
        "symfony/http-kernel": "^2.8|^3.3|^4.0",
        "symfony/routing": "^2.8|^3.3|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": { "Beelab\\PaypalBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.4.x-dev"
        }
    }
}
